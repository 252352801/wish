/* pages/signup/signup.wxss */
page{
  background-color: #F5F5F5;
}
text {
  font-size: 16px;
}
.sigin-head {
  height: 350rpx;
  display: flex;
   align-items: center;
  justify-content:center;
}
.sigin-head image {
  height: 300rpx;
  width: 300rpx;
}

.list-contain {
  height: 110rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  border-top: 1px solid #f5f5f5;
  background-color: #fff;
}

.list-contain1 {
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  border-top: 1px solid #f5f5f5;
}

.list-title {

  padding-left: 40rpx;
}

.list-title-l {
  width: 5em;
  padding-left: 1em;
  white-space: nowrap;
}

.list-title-b {
  width: 250rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  flex: 1 1 auto;
  display: flex;
}

.list-title-r {
  padding-left: 10rpx;
  padding-right: 30rpx;
  flex: 1 1 auto;
}
