<!--pages/generateshareimg/generateshareimg.wxml-->
<view>
  <view class='share-img-container'>
    <canvas class='share-img' canvas-id="shareImg"></canvas>
  </view>
  <button type='primary' class='btn-save-img' open-type='writePhotosAlbum' onWritePhotosAlbum="saveImg" bindtap='saveImg'>保存到相册</button>
</view>